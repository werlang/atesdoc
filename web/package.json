{
  "name": "suap_declaracao_docencia_web",
  "version": "0.0.1",
  "description": "Frontend for SUAP Declaração de Docência",
  "type": "module",
  "scripts": {
    "production": "webpack && node app.js",
    "development": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --inspect=0.0.0.0 app.js",
    "dev:client": "webpack serve"
  },
  "nodemonConfig": {
    "ignore": ["public/*", "src/*"]
  },
  "author": "Pablo Werlang",
  "license": "MIT",
  "dependencies": {
    "express": "^4.19.2",
    "mustache-express": "^1.3.2",
    "needle": "^3.3.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "css-loader": "^7.1.1",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "less-loader": "^12.3.0",
    "mini-css-extract-plugin": "^2.9.0",
    "nodemon": "^3.1.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  }
}
